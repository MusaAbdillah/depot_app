<% provide(:title, @user.name) %>

<div class="container-fluid">
    
        <div class="col-sm-12 col-md-3">
            <%= render 'show_navbar' %>
        </div>
        <div class="col-sm-12 col-md-9">
            <h3>Profil</h3>
            <div class="table-responsive">
			<table class="table table-stripped">
				<tbody>
					<tr>
						<td>Nama</td>
						<td><%= @user.name %></td>
					</tr>
					<tr>
						<td>Email</td>
						<td><%= @user.email %></td>
					</tr>
					<tr>
						<td>No Handphone</td>
						<td><%= @user.phone %></td>
					</tr>
					<tr>
						<td>Jenis Kelamin</td>
						<td><%= @user.gender %></td>
					</tr>
				</tbody>
			</table>
			</div>
        </div>
    
</div>