<div class="col-md-3">
	<ul class="list group">
		<li class="list-group-item disabled"><b>Product Category</b></li>
	  	<li class="list-group-item"><a href="#">Mousepad</a></li>
	  	<li class="list-group-item"><a href="#">Mouse</a></li>
	  	<li class="list-group-item"><a href="#">Keyboard</a></li>
	</ul>
</div>
<div class="col-md-9">				
	<h1> <%= t('.title_html')%> </h1>
	<% cache ['store', Product.latest] do %>
		<% @products.each do |product| %>
			<% cache ['entry', product] do %>
				<div class="entry">
					<%= image_tag(product.image_url) %>
					<h3><%= link_to product.title, product_path(product) %></h3>
					<div class="price_line">
						<span class="price"> Rp <%= number_with_delimiter(product.price) %> </span>
						<%= button_to t('.add_html'), line_items_path(product_id: product), remote: true, class: "btn btn-danger" %>
					</div>
				</div>
			<% end %>
		<% end %>
	<% end %>
</div>